<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="Host.ChatService">
        <endpoint address="Host.ChatService"
                  binding="wsDualHttpBinding"
                  contract="Host.IChatService" >
          
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:9090/ChatService"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>